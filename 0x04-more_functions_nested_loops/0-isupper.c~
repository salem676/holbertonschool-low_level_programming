#include "main.h"
#include <stdio.h>
/**
 * main - check if variabe is uppercase.
 *
 * Return: always 0.
 *
 */
int main(void)
{
	char c;
	/**
	 * c - is the character to check.
	 *
	 * Description: this part refers to variable declaration.
	 */
	c = 'A';
	printf("%c: %d\n", c, _isupper(c));
	c = 'a';
	printf("%c: %d\n", c, _isupper(c));
	return (0);
}
