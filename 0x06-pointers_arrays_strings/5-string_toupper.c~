#include <stdio.h>
#include "main.h"
/**
 * string_toupper - low to upper.
 * @s: string to modify.
 * Description: this function transforms lowercase to uppercase.
 * Return: *char.
 */
char *string_toupper(char *s)
{
	int i;
	/**
	 * @i: counter.
	 */
	i = 0;
	while (s[i] != '\0')
	{
		if ((s[i] != 96) && (s[i] < 123))
		{
			s[i] = s[i] - 32;
		}
		i++;
	}
	return (s);
}
